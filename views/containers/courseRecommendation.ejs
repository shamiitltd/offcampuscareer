                <% } %>
                <% } %>
                <% if(courses.eng[courses.eng.types['data'][ssvar.eng]].length-4>0 ) {%>
                <span class="rsub-topic"><a href="/courses/engineering/<%-courses.eng.types["path"][ssvar.eng]%>">
                        <i class="fas fa-angle-double-right"></i>
                        <%-  courses.eng[courses.eng.types['data'][ssvar.eng]].length-4+' more courses' %></a></span>
