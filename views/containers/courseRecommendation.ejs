        <div class="job-types flex-column">
            <h3 class="job-title">Engineering Colleges</h3>
            <div class="item-bar level1">
                <% for(let i=0; i<courses.eng.types["name"].length; i++) {%>
                <% if(i===ssvar.eng){ %>
                <span class="major-topic topic-selected" data-ctype="eng"
                    data-id="<%-i%>"><%-courses.eng.types["name"][i] %></span>
                <% }else{ %>
                <span class="major-topic" data-ctype="eng" data-id="<%-i%>"><%-courses.eng.types["name"][i] %></span>
