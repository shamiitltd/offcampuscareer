<form class="flex-column full-width-normal" action="/postScholarship" method="post">
    <h2 class="full-width-normal ">Scholarship Basic Info</h2>
    <div class="flex-row full-width-normal bottom-border">
        <div class="half-width-normal">
            <label for="aoi">Enter Required Qualities (Multiple select allowed) </label>
        </div>
        <div class="half-width-normal">
            <div class="chosen-container">
                <div class="chosen-choices flex-row flex-wrap" id="aoi">
                    <% if(dataObject.aoi){ %>
                    <% for(let i=0; i < dataObject.aoi.length; i++ ) {%>
                    <li class="search-choice" data-key="aoi">
                        <span><%-dataObject.aoi[i] %></span>
                        <input class="invisible" type="text" value="<%-dataObject.aoi[i] %>" name="aoi">
                        <a class="search-choice-close fas fa-times" title="Remove this tag"
                            data-val="<%-dataObject.aoi[i] %>"></a>
                    </li>
                    <% } %>
                    <% } %>
                    <!-- <input type="text" placeholder="e.g. Computer Science" class="invisible"  name="aoi" value="" data-value="" autocomplete="none"> -->
                    <li class="search-field">
                        <input type="text" placeholder="e.g. Female, Any graduation" class="searchInput"
                            autocomplete="off">
                    </li>
                </div>
                <div class="search-suggestions ">
                    <div class="autocom-box">
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="flex-row full-width-normal bottom-border">
        <div class="half-width-normal">
            <label for="companies">Enter Organisation name (Select only one) </label>
        </div>
        <div class="half-width-normal">
            <div class="chosen-container">
                <div class="chosen-choices flex-row flex-wrap" id="companies">
                    <% if(dataObject.companies){ %>
                    <li class="search-choice" data-key="companies">
                        <span><%-dataObject.companies %></span>
                        <input class="invisible" type="text" value="<%-dataObject.companies %>" name="companies">
                        <a class="search-choice-close fas fa-times" title="Remove this tag"
                            data-val="<%-dataObject.companies %>"></a>
                    </li>
                    <% } %>
                    <!-- <input type="text" placeholder="e.g. Computer Science" class="invisible"  name="companies" value="" data-value="" autocomplete="none"> -->
                    <li class="search-field">
                        <input type="text" placeholder="e.g. Google, Microsoft" class="searchInput" autocomplete="off">
                    </li>
                </div>
                <div class="search-suggestions ">
                    <div class="autocom-box">
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="flex-row full-width-normal bottom-border">
        <div class="half-width-normal">
            <label for="qualification">Enter Highest Qualification (Multiple select allowed) </label>
        </div>
        <div class="half-width-normal">
            <div class="chosen-container">
                <div class="chosen-choices flex-row flex-wrap" id="qualification">
                    <% if(dataObject.qualification){ %>
                    <% for(let i=0; i < dataObject.qualification.length; i++ ) {%>
                    <li class="search-choice" data-key="qualification">
                        <span><%-dataObject.qualification[i] %></span>
                        <input class="invisible" type="text" value="<%-dataObject.qualification[i] %>"
                            name="qualification">
                        <a class="search-choice-close fas fa-times" title="Remove this tag"
                            data-val="<%-dataObject.qualification[i] %>"></a>
                    </li>
                    <% } %>
                    <% } %>

                    <li class="search-field">
                        <input type="text" placeholder="Select Highest Qualification" class="searchInput"
                            autocomplete="off">
                    </li>
                </div>
                <div class="search-suggestions">
                    <div class="autocom-box">
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="flex-row full-width-normal bottom-border">
        <div class="half-width-normal">
            <label for="jobTitle">Enter Scholarship Title </label>
        </div>
        <div class="half-width-normal">
            <div class="chosen-container">
                <div class="chosen-choices flex-row flex-wrap" id="jobTitle">
                    <% if(dataObject.jobTitle){ %>
                    <li class="search-choice" data-key="jobTitle">
                        <span><%-dataObject.jobTitle %></span>
                        <input class="invisible" type="text" value="<%-dataObject.jobTitle %>" name="jobTitle">
                        <a class="search-choice-close fas fa-times" title="Remove this tag"
                            data-val="<%-dataObject.jobTitle %>"></a>
                    </li>
                    <% } %>

                    <li class="search-field">
                        <input type="text" placeholder="Scholarship Title" class="searchInput" autocomplete="off">
                    </li>
                </div>
                <div class="search-suggestions">
                    <div class="autocom-box">
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="flex-row full-width-normal bottom-border">
        <div class="half-width-normal">
            <label for="currency">Enter currency </label>
        </div>
        <div class="half-width-normal">
            <select class="select-box select-margin3p" id="currency" name="currency">
                <% for(let i=0; i < suggestions.currency.length; i++) {%>
                <% if(suggestions.currency[i] == dataObject.currency){ %>
                <option selected="selected" value="<%- suggestions.currency[i] %>"><%- suggestions.currency[i] %>
                </option>
                <% }else{ %>
                <option value="<%- suggestions.currency[i] %>"><%- suggestions.currency[i] %></option>
                <% } %>
                <% } %>
            </select>
        </div>
    </div>
    <div class="flex-row full-width-normal bottom-border">
        <div class="half-width-normal">
            <label for="minSalary">Enter Min Amount </label>
        </div>
        <div class="half-width-normal">
            <div class="flex-row sub-container-salary">
                <input autocomplete="none" placeholder="Min Amount" class="salary-input select-box"
                    data-initial-value="" id="minSalary" name="minSalary" type="number"
                    value="<%- dataObject.minSalary?dataObject.minSalary:'' %>">
                <span class="salary-plus-sign">+</span>
            </div>

        </div>
    </div>
    <div class="flex-row full-width-normal bottom-border">
        <div class="half-width-normal">
            <label for="maxSalary">Enter Max Amount </label>
        </div>
        <div class="half-width-normal">
            <div class="flex-row sub-container-salary">
                <input autocomplete="none" placeholder="Max Amount" class="salary-input select-box"
                    data-initial-value="" id="maxSalary" name="maxSalary" type="number"
                    value="<%- dataObject.maxSalary? dataObject.maxSalary:'' %>">
                <span class="salary-plus-sign">/-</span>
            </div>
        </div>
    </div>
    <div class="flex-row full-width-normal bottom-border">
        <div class="half-width-normal ">
            <div class="full-width-normal flex-row ">
                <div class="half-width-normal">
                    <label for="g">Government</label>
                </div>
                <div class="half-width-normal">
                    <input type="checkbox" class="s-checkbox" id="g" name="g" data-value="false" value="Government"
                        <%-dataObject.g?'checked':''  %>>
                </div>
            </div>
        </div>
        <div class="half-width-normal">
            <div class="full-width-normal flex-row">
                <div class="half-width-normal">
                    <label for="quickApply">Enter quick Apply </label>
                </div>
                <div class="half-width-normal">
                    <input type="checkbox" name="quickApply" id="quickApply" placeholder="quickApply"
                        value="Quick Apply" <%-dataObject.quickApply?'checked':''  %>>
                </div>
            </div>
        </div>

    </div>
    <div class="flex-row full-width-normal bottom-border">
        <div class="half-width-normal">
            <label for="startDate">Enter Start Date of application </label>
        </div>
        <div class="half-width-normal">
            <input type="date" name="startDate" id="startDate"
                value="<%-dataObject.lastDate? dataObject.startDate.toISOString().split('T')[0]:'' %>"
                placeholder="Start Date" class="full-wrap">
        </div>
    </div>
    <div class="flex-row full-width-normal bottom-border">
        <div class="half-width-normal">
            <label for="lastDate">Enter last Date of application <span class="red">*</span> </label>
        </div>
        <div class="half-width-normal">
            <input type="date" name="lastDate" id="lastDate"
                value="<%-dataObject.lastDate? dataObject.lastDate.toISOString().split('T')[0]:'' %>"
                placeholder="lastDate" class="full-wrap" required>
        </div>
    </div>
    <div class="flex-row full-width-normal bottom-border">
        <div class="half-width-normal">
            <label for="responseRate">Enter response Rate (Between 1 to 5) </label>
        </div>
        <div class="half-width-normal">
            <input type="number" name="responseRate" id="responseRate" value="<%-dataObject.responseRate %>"
                placeholder="responseRate" class="full-wrap">
        </div>
    </div>
    <div class="flex-row full-width-normal bottom-border">
        <div class="half-width-normal">
            <label for="logoUrlVal">Enter Logo Url <span class="red">*</span> </label>
        </div>
        <div class="half-width-normal">
            <input type="url" name="logoUrl" id="logoUrlVal" value="<%-dataObject.logoUrl %>" placeholder="Logo Url"
                class="full-wrap" required>
            <div class="company-logo flex-row">
                <img id="loadUrlImg" src="<%-dataObject.logoUrl %>" alt="No Url" class="company-logo-img">
            </div>
        </div>
    </div>

    <div class="flex-row full-width-normal bottom-border">
        <div class="half-width-normal">
            <label for="country">Enter country name </label>
        </div>
        <div class="half-width-normal">
            <input type="text" name="country" id="country" value="<%-dataObject.country %>" placeholder="country"
                class="full-wrap">
        </div>
    </div>
    <div class="flex-row full-width-normal bottom-border">
        <div class="half-width-normal">
            <label for="city">Enter city name </label>
        </div>
        <div class="half-width-normal">
            <input type="text" name="city" id="city" value="<%-dataObject.city %>" placeholder="city" class="full-wrap">
        </div>
    </div>

    <div class="flex-row full-width-normal">
        <div class="half-width-normal">
            <h2 class="full-width-normal">Scholarship Details</h2>
        </div>
        <div class="half-width-normal flex-row">
            <div class="half-width-normal">
                <span>Standard / Advance (Method) </span>
            </div>
            <div class="half-width-normal">
                <label class="switch">
                    <input id="toggleBAM" type="checkbox" checked>
                    <span class="slider round"></span>
                </label>
            </div>
        </div>
    </div>
    <div id="basicJD" class="full-width-normal" hidden>
        <%- include("./jobDescription/jdBasic", {dataObject}) %>
    </div>
    <div id="advanceJD" class="full-width-normal">
        <%- include("./jobDescription/jdAdvance", {dataObject}) %>
    </div>
    <div class="flex-row full-width-normal bottom-border" id="organizationTypeB">
        <div class="half-width-normal">
            <label for="organizationType">Enter Organization Type (optional) </label>
        </div>
        <div class="half-width-normal">
            <input type="text" name="organizationType" id="organizationType" placeholder="Organization Type"
                class="full-wrap" value="<%-dataObject.organizationType %>">
        </div>
    </div>
    <div class="flex-row full-width-normal bottom-border">
        <div class="half-width-normal">
            <label for="websiteUrlVal">Enter Website Url (or About You) <span class="red">*</span></label>
        </div>
        <div class="half-width-normal">
            <input type="url" name="websiteUrl" id="websiteUrlVal" value="<%-dataObject.websiteUrl %>"
                placeholder="Website Url" class="full-wrap" required>
        </div>
    </div>

    <div class="flex-row full-width-normal bottom-border">
        <div class="half-width-normal">
            <label for="applyUrl">Enter Apply Url <span class="red">*</span></label>
        </div>
        <div class="half-width-normal">
            <input type="text" name="applyUrl" id="applyUrl" placeholder="Apply or Quick Apply Url" class="full-wrap"
                value="<%-dataObject.applyUrl %>" required>
        </div>
    </div>
    <div class="flex-row full-width-normal bottom-border">
        <div class="half-width-normal">
            <label for="tipsUrl">Enter Useful information Url <span class="red">*</span></label>
        </div>
        <div class="half-width-normal">
            <input type="text" name="tipsUrl" id="tipsUrl" placeholder="Tips Url" class="full-wrap"
                value="<%-dataObject.tipsUrl %>" required>
        </div>
    </div>
    <% if(dataObject._id){ %>
    <input class="invisible" type="text" value="<%-dataObject._id %>" name="id">
    <% } %>
    <div class="flex-row">
        <button type="submit">Submit</button>
    </div>
</form>